{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/slideshow",
  "version": "1.0",
  "title": "Slideshow",
  "category": "widgets",
  "icon": "smiley",
  "description": "A slideshow block for reviews which uses slide blocks to create content.",
  "example": {},
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "interval": {
      "type": "number",
      "default": 3
    }
  },
  "textdomain": "slideshow",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": [
    "file:./style-index.css",
    "file:./view.css"
  ],
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}